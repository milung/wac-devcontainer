{{>partial_header}}
package {{packageName}}

import (
		"github.com/gin-gonic/gin"
)


func AddRoutes(engine *gin.Engine) *gin.RouterGroup{
	group := engine.Group("/{{{basePathWithoutHost}}}")
	{{#apiInfo}}{{#apis}}
	{
		api := new{{classname}}()
		api.addRoutes(group)
	}
	{{/apis}}{{/apiInfo}}

	return group
}
